{
  "name": "webapp-boilerplate",
  "version": "1.0.0",
  "description": "A simple SiteVision WebApp boilerplate",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitevision/webapp-boilerplate.git"
  },
  "scripts": {
    "setup": "node util/setup.js",
    "zip": "node util/zip.js",
    "deploy": "npm run zip && node util/deploy.js",
    "simpledeploy": "node util/deploy.js",
    "simpledeploysigned": "node util/deploysigned.js",
    "forcedeploy": "node util/deploy.js force",
    "forcedeploysigned": "node util/deploysigned.js force",
    "devploy": "npm run zip && node util/deploy-http.js",
    "force-deploy": "npm run zip && node util/deploy.js force",
    "create-addon": "node util/create-addon.js",
    "sign": "npm run zip && node util/sign.js",
    "simplesign": "node util/sign.js",
    "prod-deploy": "node util/prod-deploy.js",
    "manifestupdate": "node util/updateManifestVersion.js"
  },
  "license": "MIT",
  "author": "SiteVision AB",
  "bugs": {
    "url": "https://github.com/sitevision/webapp-boilerplate/issues"
  },
  "homepage": "https://github.com/sitevision/webapp-boilerplate#readme",
  "devDependencies": {
    "inquirer": "^5.2.0",
    "jsonfile": "^4.0.0",
    "request": "^2.88.0",
    "zip-dir": "^1.0.2"
  }
}
